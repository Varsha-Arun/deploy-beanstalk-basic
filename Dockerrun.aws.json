{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "$DEPLOYEBBASICIMAGE_SOURCENAME:$DEPLOYEBBASICIMAGE_VERSIONNAME",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "$DEPLOYEBENVPARAMS_PORT"
    }
  ],
  "environment": [
    {
      "name": "ENVIRONMENT",
      "value": "$DEPLOYEBENVPARAMS_ENVIRONMENT"
    },
    {
      "name": "PORT",
      "value": "$DEPLOYEBENVPARAMS_PORT"
    }
  ]
}
